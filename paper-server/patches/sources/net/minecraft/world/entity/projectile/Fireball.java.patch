diff --git a/net/minecraft/world/entity/projectile/Fireball.java b/net/minecraft/world/entity/projectile/Fireball.java
index 99aa3b854db13b4d9404b519d5e2d61a2b004b20..66dcd3c43bab5f18f8a3a891df2ff0612d5b6e6b 100644
